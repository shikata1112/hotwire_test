<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<%= image_tag @post.front_id_image.variant(resize_to_limit: [100, 100]) %>
<%= image_tag @post.back_id_image.variant(resize_to_limit: [100, 100]) %>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
